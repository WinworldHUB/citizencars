CREATE TABLE "cars" (
  "Sr. No." int NOT NULL AUTO_INCREMENT, 
  "Brand" varchar(100) NOT NULL, 
  "Model" varchar(100) NOT NULL, 
  "History" varchar(100) NOT NULL, 
  "Owner" varchar(100) NOT NULL, 
  "Kilometer Driven" varchar(100) NOT NULL, 
  "Last Service" varchar(100) NOT NULL, 
  "Registration" varchar(100) NOT NULL, 
  "Insurance" varchar(100) NOT NULL, 
  "Registered In" int NOT NULL, 
  "Fuel Type" varchar(100) NOT NULL, 
  "Engine Displacement (cc)" int NOT NULL, 
  "No. of cylinder" int NOT NULL, 
  "Max Power (bhp@rpm)" varchar(100) NOT NULL, 
  "Seating Capacity" varchar(100) NOT NULL, 
  "TransmissionType" varchar(100) NOT NULL, 
  "Body Type" varchar(100) NOT NULL, 
  "Anti Lock Braking System" varchar(100) NOT NULL, 
  "Air Conditioner" varchar(100) NOT NULL, 
  "Driver Airbag" varchar(100) NOT NULL, 
  "Passenger Airbag" varchar(100) NOT NULL, 
  "Alloy Wheels" varchar(100) NOT NULL, 
  "Multi-function Steering Wheel" varchar(100) NOT NULL, 
  "Displacement (cc)" int NOT NULL, 
  "Max Power" varchar(100) NOT NULL, 
  "Max Torque" varchar(100) NOT NULL, 
  "Valves Per Cylinder" int NOT NULL, 
  "Gear Box" varchar(100) NOT NULL, 
  "Mild Hybrid" varchar(100) NOT NULL, 
  "Steering Type" varchar(100) NOT NULL, 
  "Steering Column" varchar(100) NOT NULL, 
  "Heater" varchar(100) NOT NULL, 
  "Accessory Power Outlet" varchar(100) NOT NULL, 
  "Rear AC Vents" varchar(100) NOT NULL, 
  "Cruise Control" varchar(100) NOT NULL, 
  "Foldable Rear Seat" varchar(100) NOT NULL, 
  "Additional Features Comfort" varchar(100) NOT NULL, 
  "Fabric Upholstery" varchar(100) NOT NULL, 
  "Outside Temperature Display" varchar(100) NOT NULL, 
  "Driving Experience Control Eco" varchar(100) NOT NULL, 
  "Height Adjustable Driver Seat" varchar(100) NOT NULL, 
  "Additional Features Interior" varchar(100) NOT NULL, 
  "Power Adjustable Exterior Rear View Mirror" varchar(100) NOT NULL, 
  "Electric Folding Rear View Mirror" varchar(100) NOT NULL, 
  "Outside Rear View Mirror Turn Indicators" varchar(100) NOT NULL, 
  "Alloy Wheel Size" int NOT NULL, 
  "Tyre Size" varchar(100) NOT NULL, 
  "Additional Features Exterior" varchar(100) NOT NULL, 
  "Anti-Lock Braking System" varchar(100) NOT NULL, 
  "Brake Assist" varchar(100) NOT NULL, 
  "Central Locking" varchar(100) NOT NULL, 
  "Power Door Locks" varchar(100) NOT NULL, 
  "No of Airbags" int NOT NULL, 
  "Vehicle Stability Control System" varchar(100) NOT NULL, 
  "EBD" varchar(100) NOT NULL, 
  "Advance Safety Features" varchar(100) NOT NULL, 
  "Knee Airbags" varchar(100) NOT NULL, 
  "Hill Assist" varchar(100) NOT NULL, 
  "Radio" varchar(100) NOT NULL, 
  "Speakers Front" varchar(100) NOT NULL, 
  "Speakers Rear" varchar(100) NOT NULL, 
  "Integrated 2DIN Audio" varchar(100) NOT NULL, 
  "USB & Auxiliary input" varchar(100) NOT NULL, 
  "Bluetooth Connectivity" varchar(100) NOT NULL, 
  "Touch Screen" varchar(100) NOT NULL, 
  "Touch Screen size" varchar(100) NOT NULL, 
  "Connectivity" varchar(100) NOT NULL, 
  "Android Auto" varchar(100) NOT NULL, 
  "Apple CarPlay" varchar(100) NOT NULL, 
  "Additional Features Entertainment" varchar(100) NOT NULL, 
  PRIMARY KEY ("Sr. No.")
)
